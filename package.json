    "react-youtube": "^7.9.0",
    "sharp": "^0.25.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1",
    "typography": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
