{
  "name": "petitcode-gatsby",
  "private": true,
  "description": "petitcode gatsby website",
  "version": "1.0.0",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "develop": "nodemon -x 'gatsby develop' -w './*.js'",
    "production": "gatsby build && gatsby serve",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "test": "npm run lint && gatsby clean && npm run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
