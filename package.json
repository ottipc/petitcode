    "csv": "^3.1.0",
    "d3": "^5.15.0",
    "dotenv": "^8.2.0",
    "final-form": "^4.18.6",
    "gatsby": "^2.19.12",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-hubspot": "^1.3.3",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.30",
