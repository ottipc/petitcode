    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-contentful": "^2.1.94",
    "gatsby-transformer-sharp": "^2.3.14",
    "gatsby-transformer-sqip": "^2.1.64",
    "i18next": "^17.0.7",
    "prop-types": "^15.7.2",
    "ptz-i18n": "^1.0.0",
    "query-string": "^6.9.0",
