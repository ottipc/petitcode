{
  "name": "petitcode-gatsby",
  "private": true,
  "description": "petitcode gatsby website",
  "version": "1.0.0",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "build": "NODE_ENV=production gatsby build",
    "develop": "nodemon -x 'gatsby develop' -w './*.js'",
    "production": "gatsby build && gatsby serve",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "test": "npm run lint && rm -rdf .cache public && npm run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.18",
    "@mdx-js/react": "^1.0.16",
    "@researchgate/react-intersection-observer": "^1.0.0",
    "final-form": "^4.12.0",
    "gatsby": "^2.4.3",
    "gatsby-image": "^2.0.41",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-plugin-webpack-size": "0.0.3",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "gatsby-transformer-sqip": "^2.0.35",
    "i18next": "^15.1.0",
    "prop-types": "^15.7.2",
    "ptz-i18n": "^1.0.0",
    "react": "^16.8.6",
    "react-cookie-consent": "^2.3.1",
    "react-dom": "^16.8.6",
    "react-final-form": "^4.1.0",
    "react-final-form-html5-validation": "^1.0.3",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.9.0",
    "react-slick": "^0.24.0",
    "react-typography": "^0.16.19",
    "react-youtube": "^7.9.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.2.0",
    "typography": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "gatsby-cli": "^2.5.13",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0"
  }
}
