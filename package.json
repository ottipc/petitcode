{
  "name": "petitcode-gatsby",
  "private": true,
  "description": "petitcode gatsby website",
  "version": "1.0.0",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "build": "NODE_ENV=production gatsby build",
    "develop": "nodemon -x 'gatsby develop' -w './*.js'",
    "production": "gatsby build && gatsby serve",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "test": "npm run lint && rm -rdf .cache public && gatsby build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "@researchgate/react-intersection-observer": "^0.7.4",
    "core-js": "^2.6.5",
    "gatsby": "^2.3.7",
    "gatsby-image": "^2.0.37",
    "gatsby-mdx": "^0.4.4",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-subfont": "^1.0.5",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-plugin-webpack-size": "0.0.3",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "intersection-observer": "^0.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-cookie-consent": "^2.2.2",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-slick": "^0.23.2",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.2.0",
    "typography": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.1",
    "eslint-plugin-standard": "^4.0.0",
    "gatsby-cli": "^2.5.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
